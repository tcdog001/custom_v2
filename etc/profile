# /etc/profile: system-wide .profile file for the Bourne shells
#
#

PATH="/bin:/usr/bin:/sbin:/usr/sbin:/usr/local/sbin"
export PATH

. /etc/env.in
#. /etc/cp1.in
#. /etc/cp2.in

ulimit -c 0

# ANSI COLORS
CRE="[K"
NORMAL="[0;39m"
RED="[1;31m"
GREEN="[1;32m"
YELLOW="[1;33m"
BLUE="[1;34m"
MAGENTA="[1;35m"
CYAN="[1;36m"
WHITE="[1;37m"

umask 077

echo "${GREEN}Welcome to Autelan Linux.${NORMAL}"

